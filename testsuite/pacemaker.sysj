{/* LRI CD */
	input signal AS;
	input signal VS;
/*    input signal TLRITAVI;*/
	signal TLRITAVI;
	input channel VP;

	output signal resetTimer;
	output channel AP;
	output signal oAP;
	signal sVP;

/*    Observer signals -> */
	output channel OB;
	{
			emit resetTimer;
			while(true){
				if(TRLITAVI){
					AP!;
					emit oAP;
				}
				if(AS){
					abort(sVP||VS){
						while(true){pause;}
					}
					emit resetTimer;
					pause;
				}
				if(sVP||VS){
					emit resetTimer;
				}
				pause;
			}
			||
			while(true){
				VP?;
				emit sVP;
				pause;
			}
	}
	||
	{
/*        Environment*/
		pause;
		pause;
		pause;
		emit TLRITAVI;
	}
