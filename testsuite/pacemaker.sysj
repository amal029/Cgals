{/* AVI CD */
	input signal AS;
	input signal VS;
	input channel AP;
	signal TAVI;
	signal TURI;

	output signal resetTimer;
	output signal oVP;
	output channel VP;

	signal sAP;
/*    Observer signals -> */
	signal sus2;
	signal mytest;
	input channel OB;
	{
			while(true){
				abort(AS||sAP){while(true)pause;}
				emit resetTimer;

				abort(TAVI){while(true)pause;}
				if(TURI){
					VP!;
					emit oVP;
				}
				else{
					abort(VS){
						abort(TURI){while(true)pause;}
						VP!;
						emit oVP;
					}
				}
				pause;
			}
			||
			while(true){
				AP?;
				emit sAP;
				pause;
			}
	}
	||
	{
/*        Environment*/
		abort(sAP){while(true)pause;}
		pause;
		pause;
		pause;
		pause;
		emit TAVI; emit TURI;
	}
